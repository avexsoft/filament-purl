{
    "name": "avexsoft\/filament-purl",
    "description": "CRUD permalinks (also known as persistent URLs) to your filament project",
    "license": "MIT",
    "authors": [
        {
            "name": "avexsoft"
        }
    ],
    "homepage": "https:\/\/github.com\/avexsoft\/filament-purl",
    "keywords": [
        "Laravel",
        "FilamentPurl"
    ],
    "require": {
        "filament/filament": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0"
    },
    "autoload": {
        "psr-4": {
            "Avexsoft\\FilamentPurl\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avexsoft\\FilamentPurl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avexsoft\\FilamentPurl\\FilamentPurlServiceProvider"
            ],
            "aliases": {
                "FilamentPurl": "Avexsoft\\FilamentPurl\\Facades\\FilamentPurl"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https:\/\/satis.avexcode.com"
        }
    ]
}